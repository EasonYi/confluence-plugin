{
  "name": "confluence-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build-lite": "VERSION=v1.1.4 PRODUCT_TYPE=lite vue-cli-service build",
    "build-full": "VERSION=v1.1.4 PRODUCT_TYPE=full vue-cli-service build",
    "build-lite-win": "set VERSION=v1.1.4 && set PRODUCT_TYPE=lite && vue-cli-service build",
    "build-lite-full": "set VERSION=v1.1.4 && set PRODUCT_TYPE=full && vue-cli-service build",
    "sync": "aws s3 sync ./dist/v1.1.4 s3://atlassian.zenuml.com/v1.1.4",
    "upload-lite-descriptor": "aws s3 cp ./atlassian-connect-lite.json s3://atlassian.zenuml.com/v1.1.4/atlassian-connect-lite.json",
    "upload-full-descriptor": "aws s3 cp ./atlassian-connect.json s3://atlassian.zenuml.com/v1.1.4/atlassian-connect.json",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "sequence-parser": "^0.5.13",
    "split.js": "^1.3.5",
    "test-lib": "^2.1.2",
    "vue": "^2.5.16",
    "vue-codemirror": "^4.0.0",
    "vue-sequence": "^0.7.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.2",
    "@vue/cli-service": "^3.0.0-rc.2",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
